{
  "homepage": "https://ui.crossplatformkorea.com",
  "publishConfig": {
    "access": "public"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "predeploy": "yarn build",
    "postinstall": "node bin/greeting.js",
    "publish": "yarn build && cd lib && npm publish && cd ..",
    "lint": "eslint .",
    "lint:ci": "commitlint --from HEAD~0 --to HEAD --verbose",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --runInBand",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --android",
    "tsc": "tsc",
    "start:reset": "watchman watch-del-all && lsof -t -i :8081 | xargs kill && expo start -c",
    "jest": "jest --runInBand",
    "test:all": "yarn lint && yarn tsc && yarn test",
    "prebuild": "find ./lib -mindepth 1 ! -name 'package.json' -exec rm -rf {} +",
    "build": "tsc --project tsconfigBuild.json && cpx './src/**/*.{ts,tsx}' ./lib/",
    "postbuild": "cp ./src/react-native.config.cjs ./lib/react-native.config.cjs && rm -rf ./lib/tsconfig* && cp README.md ./lib/README.md && mkdir -p ./lib/components/uis/Icon && find ./src/components/uis/Icon/ -type f \\( -name '*.otf' -o -name '*.ttf' -o -name '*.json' \\) -exec cp {} ./lib/components/uis/Icon/ \\;",
    "storybook:build": "storybook build --docs -o ./doc-build -c ./.storybook",
    "storybook:serve": "npx http-server ./doc-build",
    "export": "expo export -p web",
    "deploy:storybook": "gh-pages -t -d dist",
    "prepare": "husky && husky install"
  },
  "dependencies": {
    "@emotion/native": "^11.11.0",
    "@emotion/react": "^11.14.0",
    "@expo/match-media": "^0.4.0",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "expo": "~52.0.23",
    "expo-constants": "~17.0.3",
    "expo-font": "~12.0.7",
    "expo-linear-gradient": "~13.0.0",
    "expo-screen-orientation": "~7.0.0",
    "expo-status-bar": "~2.0.0",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.6",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.8.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.13",
    "react-native-web-hooks": "^3.0.2",
    "react-responsive": "^10.0.0",
    "react-useinterval": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.23.3",
    "@chromatic-com/storybook": "^3.2.3",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@emotion/jest": "^11.13.0",
    "@expo/metro-runtime": "~4.0.0",
    "@jest/types": "^29.6.3",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-backgrounds": "^8.4.7",
    "@storybook/addon-controls": "^8.4.7",
    "@storybook/addon-docs": "^7.6.13",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-ondevice-actions": "^8.4.3",
    "@storybook/addon-ondevice-backgrounds": "^8.4.3",
    "@storybook/addon-ondevice-controls": "^8.4.3",
    "@storybook/addon-ondevice-notes": "^8.4.3",
    "@storybook/addon-react-native-web": "^0.0.26",
    "@storybook/addon-toolbars": "^8.4.7",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/addons": "^7.6.12",
    "@storybook/core": "^8.4.7",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^8.4.7",
    "@storybook/react-native": "^8.4.3",
    "@storybook/react-webpack5": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/jest-plugin-context": "^2.9.7",
    "@types/node": "^20.11.15",
    "@types/react": "~18.3.0",
    "@types/react-dom": "~18.2.25",
    "@types/react-responsive": "^8.0.8",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.13",
    "babel-preset-expo": "^12.0.4",
    "babel-preset-react-app": "^10.0.1",
    "commitlint-plugin-function-rules": "^4.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.1.1",
    "givens": "^1.3.9",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.2",
    "jest-plugin-context": "^2.9.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.2.0",
    "storybook": "^8.4.7",
    "storybook-dark-mode": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  }
}
